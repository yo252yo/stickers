{
  "name": "Yo Stickers!!",
  "permissions": [ "clipboardWrite", "clipboardRead", "webRequestBlocking", "tabs"],
  
  "description": "my stickers ;)",
  "version": "1.1",
  "manifest_version": 2,
  "options_page": "options.html",
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "content_scripts": [
  {
    "matches": ["http://*/*", "https://*/*"],
    "js": ["jquery.js","stickers_div.js"],
    "run_at": "document_idle"
  }],
"key": ""
}
